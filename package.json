{
  "name": "kicad_viewer",
  "version": "0.0.15",
  "description": "Visualize and explore KiCAD files in Nextcloud.",
  "main": "src/js/main.mjs",
  "private": true,
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack --config $(pwd)/.webpack.common.mjs",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --config $(pwd)/.webpack.common.mjs",
    "app:cycle": "rm -f $(pwd)/../../../../logs/nextcloud/nextcloud.log && npm run app:disable && npm run app:updatemime && npm run app:enable",
    "app:disable": "php $(pwd)/../../occ app:disable kicad_viewer",
    "app:enable": "php $(pwd)/../../occ app:enable kicad_viewer",
    "app:updatemime": "php $(pwd)/../../occ maintenance:mimetype:update-js && php $(pwd)/../../occ maintenance:mimetype:update-db --repair-filecache",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pytonballoon810/kicad_viewer.git"
  },
  "author": "WARP",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/pytonballoon810/kicad_viewer/issues"
  },
  "homepage": "https://github.com/pytonballoon810/kicad_viewer#readme",
  "devDependencies": {
    "@babel/core": "^7.27.3",
    "@babel/preset-env": "^7.27.2",
    "@nextcloud/babel-config": "^1.2.0",
    "@nextcloud/browserslist-config": "^3.0.1",
    "@nextcloud/event-bus": "^3.3.2",
    "@nextcloud/logger": "^3.0.2",
    "@nextcloud/router": "^3.0.1",
    "@nextcloud/stylelint-config": "^2.3.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "core-js": "^3.42.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.7",
    "debug": "^4.4.1",
    "markdownlint": "^0.38.0",
    "normalize.css": "^8.0.1",
    "npm-check-updates": "^18.0.1",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "postcss-scss": "^4.0.9",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.5",
    "stylelint-webpack-plugin": "^5.0.1",
    "terser-webpack-plugin": "^5.3.14",
    "tweakpane": "^4.0.5",
    "uuid": "^11.1.0",
    "vue": "^2.7.14",
    "vue-loader": "^15.10.1",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
}
